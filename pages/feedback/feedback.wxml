<!--pages/feedback/feedback.wxml-->
<view class="container">
  <image class="feedback-banner" src="/images/feedback-banner.png"></image>
  <view class="faq-section">
    <view class="faq-title">常见问题</view>
    <view class="faq-list">
      <view class="faq-item" bindtap="openModal" data-title="问题1">问题1</view>
      <view class="faq-item" bindtap="openModal" data-title="问题2">问题2</view>
      <view class="faq-item" bindtap="openModal" data-title="问题3">问题3</view>
      <view class="faq-item" bindtap="openModal" data-title="问题4">问题4</view>
      <view class="faq-item" bindtap="openModal" data-title="问题5">问题5</view>
    </view>
  </view>
  <button class="submit-btn" bindtap="navigateToFeedback">提交反馈</button>
</view>